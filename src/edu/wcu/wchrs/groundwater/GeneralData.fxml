<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wcu.wchrs.groundwater.PrepController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="910.0" prefWidth="570.0">
         <children>
            <Label layoutX="170.0" layoutY="71.0" text="Names:" />
            <TextField fx:id="nameFld" layoutX="245.0" layoutY="66.0" />
            <Label layoutX="184.0" layoutY="104.0" text="Date:" />
            <TextField fx:id="dateFld" layoutX="245.0" layoutY="99.0" />
            <Label layoutX="183.0" layoutY="137.0" text="Time:" />
            <TextField fx:id="timeFld" layoutX="245.0" layoutY="132.0" />
            <Label layoutX="165.0" layoutY="167.0" text="Weather:" />
            <TextField fx:id="weatherFld" layoutX="245.0" layoutY="162.0" />
            <Label layoutX="138.0" layoutY="198.0" text="Temperature:" />
            <TextField fx:id="temperatureFld" layoutX="245.0" layoutY="193.0" />
            <Button layoutX="240.0" layoutY="273.0" mnemonicParsing="false" onAction="#pressButton" text="Continue" />
         </children>
      </Pane>
   </children>
</GridPane>
